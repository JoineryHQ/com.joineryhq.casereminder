<?xml version="1.0"?>
<extension key="casereminder" type="module">
  <file>casereminder</file>
  <name>Case Reminders</name>
  <description>CiviCRM: Case Reminders</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/twomice/civicrm-casereminder</url>
    <url desc="Documentation">https://github.com/twomice/civicrm-casereminder</url>
    <url desc="Support">https://github.com/twomice/civicrm-casereminder/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2024-03-26</releaseDate>
  <version>0.1.0</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.65</ver>
  </compatibility>
  <comments>Developed by Joinery for the CiviCRM community.</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Casereminder</namespace>
    <format>23.02.1</format>
    <angularModule>crmCasereminder</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_Casereminder_Upgrader</upgrader>
  <requires>
    <ext>org.civicoop.emailapi</ext>
    <ext>org.civicrm.search_kit</ext>
    <ext>org.civicrm.afform</ext>
  </requires>
</extension>
